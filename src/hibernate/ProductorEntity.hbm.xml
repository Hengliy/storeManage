<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.ProductorEntity" table="productor" schema="store">
        <id name="id">
            <column name="id" sql-type="smallint(5) unsigned"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="person">
            <column name="person" sql-type="varchar(10)" length="10" />
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(15)" length="15" />
        </property>
        <property name="companyTel">
            <column name="company_tel" sql-type="varchar(15)" length="15" />
        </property>
        <property name="postcode">
            <column name="postcode" sql-type="varchar(10)" length="10" />
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(30)" length="30" />
        </property>
        <many-to-one name="goodsByName" class="entity.GoodsEntity">
            <column name="name"/>
        </many-to-one>
    </class>
</hibernate-mapping>