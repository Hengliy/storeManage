<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.KindEntity" table="kind" schema="store">
        <id name="id">
            <column name="id" sql-type="smallint(5) unsigned"/>
        </id>
        <property name="name">
            <column name="name" sql-type="char(10)" length="10" not-null="true"/>
        </property>
        <property name="count">
            <column name="count" sql-type="smallint(6)" />
        </property>
        <many-to-one name="goodsByName" class="entity.GoodsEntity">
            <column name="name"/>
        </many-to-one>
    </class>
</hibernate-mapping>