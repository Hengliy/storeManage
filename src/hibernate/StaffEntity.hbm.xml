<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.StaffEntity" table="staff" schema="store">
        <id name="id">
            <column name="id" sql-type="smallint(5) unsigned"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="sex">
            <column name="sex" sql-type="char(2)" length="2" />
        </property>
        <property name="broth">
            <column name="broth" sql-type="date" />
        </property>
        <property name="tel">
            <column name="tel" sql-type="varchar(15)" length="15" />
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(32)" length="32" />
        </property>
    </class>
</hibernate-mapping>